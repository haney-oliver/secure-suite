{
  "manifest_version": 2,
  "version": "1",
  "name": "Secure Suite Browser Extension",
  "homepage_url": "http://192.168.1.165:8080/",
  "description": "Secure Suite browser extension",
  "default_locale": "en",
  "permissions": [
    "<all_urls>",
    "*://*/*",
    "webNavigation"
  ],
  "icons": {
    "16": "icons/secure-suite-logo-shadowless.svg",
    "48": "icons/secure-suite-logo-shadowless.svg",
    "128": "icons/secure-suite-logo-shadowless.svg"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Secure Suite Browser Extension",
    "default_icon": {
      "19": "icons/secure-suite-logo-shadowless.svg",
      "38": "icons/secure-suite-logo-shadowless.svg"
    }
  }
}
